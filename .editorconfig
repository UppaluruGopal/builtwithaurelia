# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# 4 space indentation
[**.*]
indent_style = space
indent_size = 4

[{webpack.config.js,package.json,typings.json,tsconfig.json,wallaby.js}]
indent_size = 2
