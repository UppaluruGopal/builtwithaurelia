{
  "name": "builtwithaurelia",
  "version": "1.0.0",
  "description": "A showcase of Aurelia plugins, themes, applications and more",
  "main": "dist/app.bundle.js",
  "repository": {
    "url": "git+ssh://git@github.com/vheissu/builtwithaurelia.git",
    "type": "git"
  },
  "author": "Dwayne Charrington <dwaynecharrington@gmail.com> (https://ilikekillnerds.com)",
  "license": "CC0-1.0",
  "scripts": {
    "test": "nps test",
    "start": "nps",
    "watch": "nps webpack.server.ssr.watch",
    "server": "nps webpack.server.ssr.start",
    "build": "nps webpack.build",
    "build:ssr": "nps webpack.build.production.ssr"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.(ts|js)x?$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.ts"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "aurelia-(.*)": "<rootDir>/node_modules/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "mapCoverage": true
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/jsdom": "^11.0.6",
    "@types/lodash": "^4.14.115",
    "@types/node": "9.6.24",
    "@types/webpack": "^4.4.8",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-protractor-plugin": "^1.0.6",
    "aurelia-testing": "^1.0.0-beta.4.0.0",
    "aurelia-webpack-plugin": "^3.0.0",
    "bluebird": "^3.5.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "ejs": "^2.6.1",
    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "imports-loader": "^0.8.0",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "jsdom": "^11.12.0",
    "koa-compress": "^3.0.0",
    "koa-html-minifier": "^1.0.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "nps": "^5.9.2",
    "nps-utils": "^1.6.0",
    "postcss-loader": "^2.1.6",
    "protractor": "^5.4.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "wait-on": "^2.1.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.0",
    "aurelia-fetch-client": "^1.4.0",
    "aurelia-middleware-koa": "^0.1.1",
    "aurelia-pal-nodejs": "^1.0.0-beta.3.2.0",
    "aurelia-polyfills": "^1.3.0",
    "aurelia-ssr-bootstrapper-webpack": "^0.1.1",
    "aurelia-store": "^0.26.0",
    "autoprefixer": "^9.0.1",
    "bootstrap": "^3.3.7",
    "firebase": "^5.3.0",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "koa": "^2.5.2",
    "koa-static": "^5.0.0",
    "node-fetch": "^2.2.0",
    "xmlhttprequest": "^1.8.0"
  }
}
